{
  "name": "armada-ui",
  "private": true,
  "version": "1.0.0",
  "description": "Admin and Agent Portal",
  "main": "dist",
  "scripts": {
    "build": "npm-run-all --parallel type-check build:js",
    "build:js": "webpack -p",
    "lint": "eslint --cache --fix --ext '.ts,.tsx,.js,.jsx' .",
    "prettier": "prettier --write \"**/*.{js,json,css,jsx,mjs,ts,tsx,yaml,yml,graphql}\"",
    "start": "webpack-dev-server --open",
    "type-check": "tsc --noEmit",
    "watch": "webpack --watch",
    "test": "jest --watch --coverage --verbose --colors "
  },
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "*.{js,json,css,jsx,mjs,ts,tsx,yaml,yml,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "<rootDir>/config/jest/styleMock.js",
      "\\.(png|jpg|gif|ttf|eot|svg)$": "<rootDir>/config/jest/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.dist/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "author": "ArmadaHealth",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-http": "^1.5.14",
    "apollo-link-ws": "^1.0.17",
    "apollo-utilities": "^1.3.1",
    "baseui": "^6.21.1",
    "casual": "^1.6.2",
    "classnames": "^2.2.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "graphql": "^14.3.0",
    "jwt-decode": "^2.2.0",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.8.6",
    "react-virtualized": "^9.21.1",
    "styletron-engine-atomic": "^1.1.2",
    "styletron-react": "^5.0.1",
    "subscriptions-transport-ws": "^0.9.16",
    "uuid": "^3.3.2",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@graphql-codegen/add": "^1.1.3",
    "@graphql-codegen/cli": "^1.1.3",
    "@graphql-codegen/core": "^1.1.3",
    "@graphql-codegen/typescript": "^1.1.3",
    "@graphql-codegen/typescript-resolvers": "^1.1.3",
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.12",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "typescript": "^3.5.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
